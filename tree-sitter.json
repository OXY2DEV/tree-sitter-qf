{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "qf",
      "camelcase": "QF",
      "title": "Quickfix",
      "scope": "source.qf",
      "file-types": [
        "qf"
      ],
      "injection-regex": "^qf$",
      "class-name": "TreeSitterQf"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter parser for Vim's quickfix list.",
    "authors": [
      {
        "name": "MD. Mouinul Hossain",
        "email": "mdmouinulhossainshawon@gmail.com",
        "url": "https://github.com/OXY2DEV/"
      }
    ],
    "links": {
      "repository": "https://github.com/OXY2DEV/tree-sitter-qf.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}